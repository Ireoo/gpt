{
  "name": "gpt",
  "version": "1.0.2",
  "author": "Ireoo <i@ireoo.com>",
  "description": "speak with git",
  "homepage": "https://github.com/ireoo/gpt",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "prebuild:windows1": "npm run build:clean && node .electron-vue/build.js",
    "prebuild:macos1": "npm run build:clean && node .electron-vue/build.js",
    "prebuild:ubuntu1": "npm run build:clean && node .electron-vue/build.js",
    "build:macos": "electron-builder --mac --x64 --publish never && node name.js x64",
    "build:windows": "electron-builder --win --x64 --publish never && node name.js x64",
    "build:ubuntu": "electron-builder --linux --x64 --publish never && node name.js x64",
    "prebuild:all": "npm run build:clean && node .electron-vue/build.js",
    "build:all": "npm run build:macos && npm run build:windows && npm run build:ubuntu",
    "prebuild": "npm run build:clean && node .electron-vue/build.js",
    "build": "electron-builder --publish never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "cross-env RUN=dev node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "version": "node package.js",
    "test": "nodemon test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run version && git add package.json && git add version && pretty-quick --staged"
    }
  },
  "build": {
    "productName": "gpt",
    "appId": "com.ireoo.gpt",
    "directories": {
      "output": "build"
    },
    "extraResources": [
      {
        "from": "node_modules/regedit/vbs",
        "to": "regedit/vbs",
        "filter": ["**/*"]
      }
    ],
    "files": ["dist/electron/**/*"],
    "extraFiles": ["command/**/*", "language/**/*", "config/**/*", "version"],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": ["dmg", "pkg", "zip"],
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": ["nsis", "zip"],
      "icon": "build/icons/icon.ico",
      "requestedExecutionLevel": "highestAvailable"
    },
    "linux": {
      "target": ["deb", "AppImage", "zip"],
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "perMachine": true
    }
  },
  "dependencies": {
    "@nexssp/os": "^2.0.18",
    "@tensorflow/tfjs": "^4.5.0",
    "axios": "^0.21.1",
    "compressing": "^1.5.1",
    "deepspeech": "^0.9.3",
    "dev-ip": "^1.0.1",
    "electron-dl": "^3.2.1",
    "electron-store": "^8.0.0",
    "element-ui": "^2.15.2",
    "express": "^4.17.1",
    "files": "^2.1.1",
    "fs-extra": "^10.0.0",
    "https-proxy-agent": "^5.0.0",
    "jquery": "^3.6.0",
    "js-base64": "^3.7.2",
    "js-yaml": "^4.1.0",
    "local-ip-url": "^1.0.3",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "node-machine-id": "^1.1.12",
    "plist": "^3.0.4",
    "progress-stream": "^2.0.0",
    "query-string": "^7.0.1",
    "regedit": "^5.0.0",
    "request": "^2.88.2",
    "socket.io-client": "^2.3.0",
    "sudo-prompt": "^9.2.1",
    "unzipper": "^0.10.11",
    "url-parse": "^1.5.3",
    "vue": "^2.6.14",
    "vue-electron": "^1.0.6",
    "vue-google-adsense": "^1.9.3",
    "vue-router": "^3.5.1",
    "vue-script2": "^2.1.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.9.3",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^24.3.0",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-gyp": "^8.1.0",
    "node-loader": "^0.6.0",
    "nodemon": "^2.0.14",
    "pkg": "^5.3.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-adsense": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
